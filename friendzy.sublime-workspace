			"	//public $access_token;\n	public $basecamp_connected = false;\n\n	// To use for CI properties\n	public $CI =& get_instance();\n\n	// Initial token for basecamp connection\n	public function _get_access_token() {\n\n		$access_token = '';\n\n		if ( isset($_GET['code']) ) {\n\n			$code = $_GET['code'];\n\n		    $vars = array(\n		        'type' => 'web_server',\n		        'client_id' => 'a7f80e8a968d9968907a7706169669f06d336be4',\n		        'redirect_uri' => 'http://localhost:8888/BlackMesa/projects',\n		        'client_secret' => '51febb26a722fa65f6e1fdf28acc8cbc1f1dbfbc',\n		        'code' => $code\n		    );\n\n			$url = 'https://launchpad.37signals.com/authorization/token';\n		    $c = curl_init($url);\n		    curl_setopt($c, CURLOPT_POST, true);\n		    curl_setopt($c, CURLOPT_POSTFIELDS, http_build_query($vars));\n		    curl_setopt($c, CURLOPT_RETURNTRANSFER, true);\n		    $response = json_decode(curl_exec($c));\n\n		    //print_r($response);\n\n		    // Get access token\n			$access_token = $response->access_token;\n			$refresh_token = $response->refresh_token;\n\n			$auth = $this->_get_basecamp_auth($access_token);\n			$basecamp_id = $auth->accounts[0]->id;\n			$expiration = $auth->expires_at;\n\n		    curl_close($c);\n		    unset($c,$url);\n\n		    // Load and set users basecamp info (access token, and basecamp ID)\n		    // Then setting the session data\n		    $this->CI->load->model('basecamp_model');\n		    $this->CI->basecamp_model->set_basecamp_data($access_token, $basecamp_id, $refresh_token, $expiration);\n		    $this->CI->session->set_userdata(array('access_token'=>$access_token, 'basecamp_id'=>$basecamp_id));\n\n		} else {\n\n			$access_token = '';\n\n		}\n\n		return $access_token;\n\n	}\n\n	//// === BASECAMP USER ID === ////\n	public function _get_basecamp_auth($access_token) {\n\n	    $url = \"https://launchpad.37signals.com/authorization.json\";\n	    $c = curl_init($url);\n\n	    curl_setopt($c, CURLOPT_HTTPHEADER, array(\n	        \"Authorization: Bearer \" . $access_token,\n	        \"Content-Type: application/json; charset=utf-8\",\n	        \"User-Agent: Cheeser (http://localhost:8888/BlackMesa/projects)\"\n	    ));\n	    curl_setopt($c, CURLOPT_RETURNTRANSFER, true);\n	    $auth = json_decode(curl_exec($c)); // reply from 37 signal auth\n	    curl_close($c);\n	    unset($c,$url);\n\n	    return $auth;\n\n	}\n\n	//// === PROJECTS === ////\n	public function _load_projects($access_token) {\n\n		$projects;\n	    \n		$url = \"https://basecamp.com/1927113/api/v1/projects.json\";\n\n		// Check database for exisiting request\n		$this->CI->load->model('basecamp_model');\n		$cached_request = $this->CI->basecamp_model->get_cached_request($url, $CI->session->userdata('basecamp_id'));\n\n		if ( $cached_request->exists && $cached_request->outdated !== true ) {\n\n			$projects = json_encode($cached_request->json);\n\n		// Curl new request from basecamp\n		} else {\n\n	    	$c = curl_init($url);\n\n		    curl_setopt($c, CURLOPT_HTTPHEADER, array(\n		        \"Authorization: Bearer \" . $access_token,\n		        \"Content-Type: application/json; charset=utf-8\",\n		        \"User-Agent: Cheeser (http://localhost:8888/BlackMesa/projects)\"\n		    ));\n		    curl_setopt($c, CURLOPT_RETURNTRANSFER, true);\n		    $projects = json_decode(curl_exec($c));\n		    curl_close($c);\n		    unset($c,$url);\n\n		    // Cache request in database\n		    $this->CI->basecamp_model->set_cached_request($url, $CI->session->userdata('basecamp_id'), $projects);\n\n		}\n\n		return $projects;\n\n	}\n\n	//// === TODOS === ////\n	public function _load_todos($access_token) {\n	    \n		$url = \"https://basecamp.com/1927113/api/v1/todolists.json\";\n	    $c = curl_init($url);\n\n	    curl_setopt($c, CURLOPT_HTTPHEADER, array(\n	        \"Authorization: Bearer \" . $access_token,\n	        \"Content-Type: application/json; charset=utf-8\",\n	        \"User-Agent: Cheeser (http://localhost:8888/BlackMesa/projects)\"\n	    ));\n	    curl_setopt($c, CURLOPT_RETURNTRANSFER, true);\n	    $projects = json_decode(curl_exec($c));\n	    curl_close($c);\n	    unset($c,$url);\n\n		return $projects;\n\n	}",
